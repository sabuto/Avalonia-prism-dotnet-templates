{
  "$schema": "http://json.schemastore.org/template",
  "author": "Sabuto",
  "classifications": [
    "Desktop",
    "Xaml",
    "Avalonia",
    "Prism",
    "Windows",
    "Linux",
    "macOS"
  ],
  "description": "A cross-platform Avalonia UI Application using the MVVM pattern with the Prism Framework",
  "groupidentity": "Avalonia.Prism.Dialog",
  "identity": "Avalonia.Prism.Dialog",
  "name": "Avalonia Prism .NET Core MVVM Dialog App",
  "preferNameDirectory": true,
  "shortName": "avalonia.prism.dialog",
  "sourceName": "PrismSimpleDialogTemplate",
  "defaultName": "PrismSimpleDialog",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0",
          "description": "Target net6.0"
        }
      ],
      "replaces": "net6.0",
      "defaultValue": "net6.0"
    },
    "MVVMToolkit": {
      "type": "parameter",
      "description": "MVVM toolkit to use in the template.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "ReactiveUI",
          "description": "Choose ReactiveUI as MVVM toolkit in the template."
        },
        {
          "choice": "CommunityToolkit",
          "description": "Choose CommunityToolkit as MVVM toolkit in the template."
        }
      ],
      "defaultValue": "ReactiveUI"
    },
    "ReactiveUIToolkitChosen": {
      "type": "computed",
      "value": "(MVVMToolkit == \"ReactiveUI\")"
    },
    "AvaloniaVersion": {
      "type": "parameter",
      "description": "The target version of Avalonia NuGet packages.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "0.10.18",
          "description": "Target 0.10.18 (Latest stable)."
        },
        {
          "choice": "11.0.0-preview4",
          "description": "Target 11.0.0-preview4"
        }
      ],
      "replaces": "AvaloniaVersionTemplateParameter",
      "defaultValue": "0.10.18"
    },
    "PrismVersion": {
      "type": "parameter",
      "description": "The target version of Prism.Avalonia NuGet packages.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "8.1.97.2",
          "description": "Target 8.1.97.2 (Latest stable)."
        }
      ],
      "replaces": "PrismVersionTemplateParameter",
      "defaultValue": "8.1.97.2"
    },
    "AvaloniaStableChosen": {
      "type": "computed",
      "value": "(AvaloniaVersion == \"0.10.18\")"
    }
  }
}